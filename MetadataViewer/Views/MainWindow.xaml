<Window x:Class="MetadataViewer.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MetadataViewer.Views"
        mc:Ignorable="d"
        Title="MetadataViewer"
        Height="450" Width="800" >

    <DockPanel Margin="10" >
        <DataGrid CanUserReorderColumns="False" CanUserSortColumns="False"
                  ItemsSource="{Binding MetaTags.Value, Mode=OneWay}" />
    </DockPanel>
</Window>
